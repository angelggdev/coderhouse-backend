html 
    head
        title="Agregar producto"
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3", crossorigin="anonymous")
    body
        div(class="container d-flex flex-column justify-content-center align-items-center", style="background-color: #CFCFEA; padding: 60px; width:90vw;")
            ul 
                li Argumentos de entrada:
                    ul
                        each arg in args 
                            li=arg
                li Nombre de la plataforma:
                    div=os
                li Versión de node.js:
                    div=nodeVersion
                li Memoria total reservada:
                    div=rss
                li Path de ejecución:
                    div=execPath
                li Process id:
                    div=pid
                li Carpeta del proyecto:
                    div=projectDir