{{#if showList}}
<table>
    <tr>
        <th>Nombre del producto</th>
        <th>Precio</th>
        <th>Imagen</th>
    </tr>
    {{#each list}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img src={{this.thumbnail}} alt={{this.title}} width='100px'></td>
        </tr>
    {{/each}}
</table>
{{else}}
<h2>No hay productos</h2>
{{/if}}
<a href='/'>Cargar productos</a>